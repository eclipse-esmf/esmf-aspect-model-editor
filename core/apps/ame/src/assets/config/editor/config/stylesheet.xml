<mxStylesheet>
  <add as="defaultEdge">
    <add as="endArrow" value="open" />
    <add as="edge" value="entityRelationEdgeStyle" />
    <add as="curved" value="1" />
    <add as="strokeColor" value="#000000" />
    <add as="fontColor" value="#000000" />
  </add>
	<add as="entityValueEntityEdge" extend="defaultEdge">
		<add as="dashed" value="1" />
	</add>
	<add as="abstractElementEdge" extend="defaultEdge">
		<add as="endArrow" value="block" />
	</add>
	<add as="optionalPropertyEdge" extend="defaultEdge">
		<add as="dashed" value="1" />
	</add>
	<add as="abstractPropertyEdge" extend="abstractElementEdge">
		<add as="dashed" value="1" />
	</add>
  <add as="common">
		<add as="fontSize" value="12" />
	</add>
	<add as="model" extend="common">
		<add as="shape" value="rectangle" />
		<add as="fontStyle" value="1" />
		<add as="align" value="center" />
		<add as="verticalAlign" value="top" />
		<add as="spacing" value="10" />
	</add>
	<add as="model_property" extend="common">
		<add as="shape" value="label" />
		<add as="align" value="left" />
		<add as="verticalAlign" value="middle" />
		<add as="spacing" value="5" />
		<add as="spacingLeft" value="5" />
		<add as="spacingRight" value="5" />
		<add as="whiteSpace" value="wrap" />
		<add as="height" value="20" />
	</add>
	<add as="aspect" extend="model">
		<add as="connectable" value="1" />
	</add>
	<add as="aspect_property" extend="model_property">
		<add as="height" value="10" />
		<add as="connectable" value="1" />
	</add>
	<add as="property" extend="model">
		<add as="connectable" value="1" />
		<add as="id" value="22" />
	</add>
	<add as="property_property" extend="model_property">
		<add as="fillColor" value="#C5C8D4" />
		<add as="connectable" value="1" />
	</add>
	<add as="operation" extend="model">
		<add as="connectable" value="1" />
		<add as="id" value="22" />
	</add>
	<add as="operation_property" extend="model_property">
		<add as="connectable" value="1" />
	</add>
	<add as="event" extend="model">
		<add as="connectable" value="1" />
	</add>
	<add as="event_property" extend="model_property">
		<add as="connectable" value="1" />
	</add>
	<add as="characteristic" extend="model">
		<add as="connectable" value="1" />
	</add>
	<add as="characteristic_property" extend="model_property">
		<add as="connectable" value="1" />
	</add>
	<add as="constraint" extend="model">
		<add as="connectable" value="1" />
	</add>
	<add as="constraint_property" extend="model_property">
		<add as="connectable" value="1" />
	</add>
	<add as="entity" extend="model">
		<add as="connectable" value="1" />
	</add>
	<add as="entity_property" extend="model_property">
		<add as="connectable" value="1" />
	</add>
	<add as="abstractEntity" extend="model">
		<add as="connectable" value="1" />
	</add>
	<add as="abstractEntity_property" extend="model_property">
		<add as="connectable" value="1" />
	</add>
	<add as="abstractProperty" extend="model">
		<add as="connectable" value="1" />
	</add>
	<add as="abstractProperty_property" extend="model_property">
		<add as="connectable" value="1" />
	</add>
  <add as="unit" extend="model">
    <add as="connectable" value="1"/>
  </add>
  <add as="unit_property" extend="model_property">
    <add as="connectable" value="1"/>
  </add>
	<add as="trait" extend="model">
		<add as="shape" value="ellipse" />
		<add as="perimeter" value="ellipsePerimeter"/>
		<add as="verticalAlign" value="middle" />
		<add as="connectable" value="1"/>
		<add as="overflow" value="width"/>
	</add>
	<add as="trait_property" extend="model_property">
		<add as="connectable" value="1"/>
	</add>
	<add as="entityValue" extend="characteristic">
		<add as="rounded" value="1"/>
		<add as="fillOpacity" value="70"/>
	</add>

	<!-- Filtered properties -->
	<add as="filteredProperties_entity" extend="trait"></add>
	<add as="filteredProperties_either" extend="trait"></add>
</mxStylesheet>
