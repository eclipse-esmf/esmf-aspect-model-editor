{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "resolveJsonModule": true,
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "module": "esnext",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "ignoreDeprecations": "5.0",
    "strictNullChecks": false,
    "esModuleInterop": true,
    "paths": {
      "@ame/api": ["libs/api/src/index.ts"],
      "@ame/app/*": ["apps/ame/src/app/*"],
      "@ame/aspect-exporter": ["libs/aspect-exporter/src/index.ts"],
      "@ame/cache": ["libs/cache/src/index.ts"],
      "@ame/connection": ["libs/connection/src/index.ts"],
      "@ame/editor": ["libs/editor/src/index.ts"],
      "@ame/instantiator": ["libs/instantiator/src/index.ts"],
      "@ame/loader-filters": ["libs/loader-filters/src/index.ts"],
      "@ame/meta-model": ["libs/meta-model/src/index.ts"],
      "@ame/mx-graph": ["libs/mx-graph/src/index.ts"],
      "@ame/namespace-manager": ["libs/namespace-manager/src/index.ts"],
      "@ame/rdf/*": ["libs/rdf/src/lib/*"],
      "@ame/settings-dialog": ["libs/settings-dialog/src/index.ts"],
      "@ame/shared": ["libs/shared/src/index.ts"],
      "@ame/shared/pipes": ["libs/shared/src/lib/pipes/index.ts"],
      "@ame/sidebar": ["libs/sidebar/src/index.ts"],
      "@ame/translation": ["libs/translation/src/index.ts"],
      "@ame/utils": ["libs/utils/src/index.ts"],
      "@esmf/aspect-model-loader": ["libs/aspect-model-loader/src/index.ts"]
    },
    "typeRoots": ["node_modules/@types", "node_modules/@typed-mxgraph"],
    "types": ["node", "jest"],
    "lib": ["es2018", "dom"]
  },
  "exclude": ["node_modules", "tmp"]
}
