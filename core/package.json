    "n3": "~1.17.1",
    "ngx-toastr": "^18.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "portfinder": "^1.0.28",
    "rxjs": "7.8.1",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.6.3",
    "typescript-mix": "^3.1.3",
    "urn-lib": "^2.0.0",
    "util": "^0.12.5",
    "zone.js": "0.14.7"
  },
