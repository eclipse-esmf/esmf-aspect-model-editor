    "@angular/platform-browser-dynamic": "18.0.4",
    "@angular/router": "18.0.4",
    "@electron/remote": "^2.1.3",
    "@juggle/resize-observer": "^4.0.0-4",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
